#include <stdio.h>
#include <malloc.h>
#include<string.h>

typedef struct stosik{
    char znak;
    struct stosik *nast;
}stos;

void dodaj(stos **head, char znak){
    stos * nowy = malloc(sizeof(stos));
    nowy -> znak = znak;
    nowy -> nast =NULL;
    if(*head == NULL){
        *head = nowy;
    }
    else{
        stos *n = *head;
        while(n -> nast !=NULL){
            n = n -> nast;
            n -> nast = nowy;
        }
        n -> nast = nowy;
        nowy -> nast =NULL;
    }
}
void usun_element_stosu(stos *head){
    stos *n = head;
    while(n-> nast != NULL){
        n = n->nast;
    }
    n -> nast = NULL;
    free(n);
}
void wypisz(stos *head){
    stos *n = head;
    while(n-> nast != NULL){
        n = n->nast;
    }
    printf("%c", n->znak);
    usun_element_stosu(head);
}

